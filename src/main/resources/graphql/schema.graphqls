
type Query {
    allUsers: [User]
    archivedUsers: [User]
    notArchivedUsers: [User]
    usersByCity(city: String!): [User]
}

type Mutation {
    createUser(name: String!, email: String!, gender: String!, archived: Boolean): User
}

type User {
    id: ID
    name: String
    email: String
    gender: String
    archived: Boolean
}