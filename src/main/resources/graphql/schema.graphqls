
type Query {
    allUsers: [User]
    archivedUsers: [User]
    notArchivedUsers: [User]
    usersByCity(city: String!): [User]
}

type Mutation {
    createUser(user: userInput): User
}

input userInput {
    name: String!
    email: String!
    gender: String!
    archived: Boolean

}

type User {
    id: ID
    name: String
    email: String
    gender: String
    archived: Boolean
    city: [String]
}